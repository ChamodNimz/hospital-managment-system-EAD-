<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.actions.Ward" table="ward">

		<id name="w_id" column="w_id">
			<generator class="native" />
		</id>

		<property name="w_name">
			<column name="w_name" length="30" not-null="true" />
		</property>

		<property name="room_count">
			<column name="room_count"  not-null="true" />
		</property>

		<property name="available_rooms">
			<column name="available_rooms"  not-null="true" />
		</property>


		<property name="price">
			<column name="price"  not-null="true" />
		</property>



	</class>
</hibernate-mapping>