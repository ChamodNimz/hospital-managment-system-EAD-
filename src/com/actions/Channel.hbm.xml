<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.actions.Channel" table="channellings">

		<id name="channel_id" column="channel_id">
			<generator class="native" />
		</id>

		<property name="doc_id">
			<column name="doc_id" not-null="true" />
		</property>

		<property name="date">
			<column name="date"  not-null="true" />
		</property>
		
		<property name="desease_type">
			<column name="desease_type"  not-null="true" />
		</property>
		
	</class>
</hibernate-mapping>